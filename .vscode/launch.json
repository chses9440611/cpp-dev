{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cpp-processing",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cpp_build/test_processing",
            "args": [
                "--gtest_filter=*.*",
                "--gtest_break_on_failure"
            ],
            "preLaunchTask": "Cpp-re-make",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "osx": {
                "MIMode": "lldb"
            }
        },
        {
            "name": "Cpp-all",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cpp_build/leetcode/test_all",
            "args": [
                "--gtest_filter=*.*",
                "--gtest_break_on_failure"
            ],
            "preLaunchTask": "Cpp-build-tests",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "osx": {
                "MIMode": "lldb"
            }
        },
        {
            "name": "Cpp-tools",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cpp_build/tools/test_tools",
            "args": [
                "--gtest_filter=*.*",
                "--gtest_break_on_failure"
            ],
            "preLaunchTask": "Cpp-build-tests",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "osx": {
                "MIMode": "lldb"
            }
        },
        {
            "name": "Python-Task",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/tasks.py",
            "args": [
                "default"
            ],
            "console": "integratedTerminal"
        },

    ]
}